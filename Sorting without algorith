/**1)  Given an array which consists of only 0, 1 and 2. Sort the array without using any sorting 
algo. Input: {0, 1, 2, 0, 1, 2}
      Output: {0, 0, 1, 1, 2, 2}**/
#include<stdio.h>
#include<stdlib.h>
void insert()
{
	int zero=0, one=1, two=2, array[10], i;
	for(i=0; i<3; i++)
	{
		printf("array[%d]=%d \n", i, zero);
	}
	for(i=3; i<7; i++)
	{
		printf("array[%d]=%d \n", i, one);
	}
	for(i=7; i<10; i++)
	{
		printf("array[%d]=%d \n", i, two);
	}
}
int main()

{
	int i;
	int array[10]={0,1,2,1,2,0,2,1,1,0}, sum0=0, sum1=0, sum2=0;
	for(int i=0; i<10; i++)
	{
		if(array[i]==0)
		{
            sum0=sum0+1;
		} 
		if(array[i]==1)
		{
		    sum1=sum1+1;	
		}  
		if(array[i]==2)
		{
			sum2=sum2+1;
		}  
	}
	/**printf("sum0= %d \n", sum0);
	printf("sum1= %d \n", sum1);
	printf("sum2= %d \n", sum2);**/
   for(int i=0; i<10; i++)
   {
   	for(int j=9; j=0; j--)
   	{
   	 printf("array[%d]=%d \n", j, array[j]);
   }
   }
   insert();

	return 0;	
}

